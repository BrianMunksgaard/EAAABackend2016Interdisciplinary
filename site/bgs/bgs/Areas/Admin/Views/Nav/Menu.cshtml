@model IEnumerable<string>

@Html.ActionLink("Admininstration", "Index", "Home", new { area = "Admin" }, new { @class = "btn btn-block btn-default btn-md" })

@*
@Html.ActionLink("Sleeves", "Index", "Home", null, new { @class = "btn btn-block btn-default btn-md" })
*@

@foreach (var link in Model)
{
    @Html.RouteLink(link, 
        new
        {
            controller = "Home",
            action = "Index",
            category = link,
            page = 1
        }, 
        new
        {
            @class = "btn btn-block btn-default btn-md" + (link == ViewBag.SelectedCategory ? " btn-primary" : "")
        })
}
